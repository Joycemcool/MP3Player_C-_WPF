<UserControl x:Class="MP3Player_Xiaoxia.DisplayingTags"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MP3Player_Xiaoxia"
             mc:Ignorable="d" Height="49" Width="178">
    <Grid Background="#FFFFFF" Margin="0,0,0,-9">
        <Label x:Name="TagHeader" Content="TagHeader" HorizontalAlignment="Center" VerticalAlignment="Top" Width="178" FontWeight="Bold"/>
        <Label x:Name="TagValue" Content="TagValue" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,31,0,0" Width="178"/>

    </Grid>
</UserControl>
